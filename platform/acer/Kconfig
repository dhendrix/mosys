#
# Acer platform configuration
#

menu "Acer Platform Support"

config PLATFORM_ACER
	bool "Acer platform support"
	default n

config ACER_CHROMIA700
	depends on PLATFORM_ARCH_X86 && PLATFORM_ACER
	select CHIPSET_INTEL_NM10
	select EC_NUVOTON_NPCE781
	bool "Acer Chromia 700 Chromebook"
	default n

endmenu
