#
# Experimental platform configuration
#

menu "Experimental Platform Support"

config PLATFORM_EXPERIMENTAL
	bool "Experimental platform support"
	default n

config EXPERIMENTAL_BELTINO
	depends on PLATFORM_ARCH_X86 && PLATFORM_EXPERIMENTAL
	select CHIPSET_INTEL_LYNXPOINT_LP
	bool "beltino"
	default n

config EXPERIMENTAL_KIEV
	depends on PLATFORM_ARCH_X86 && PLATFORM_EXPERIMENTAL
	select CHIPSET_INTEL_SERIES6
	select SUPERIO_ITE_IT8772
	bool "kiev"
	default n

config EXPERIMENTAL_RAMBI
	depends on PLATFORM_ARCH_X86 && PLATFORM_EXPERIMENTAL
	select CROS_EC_LPC
	select SOC_INTEL_BAYTRAIL
	bool "rambi"
	default n

config EXPERIMENTAL_NYAN
        depends on PLATFORM_ARCH_ARMEL && PLATFORM_EXPERIMENTAL
        select CROS_EC_SPI
        bool "nyan"
        default n

config EXPERIMENTAL_SLIPPY
	depends on PLATFORM_ARCH_X86 && PLATFORM_EXPERIMENTAL
	select CHIPSET_INTEL_LYNXPOINT_LP
	select CROS_EC_LPC
	bool "slippy"
	default n

config PLATFORM_SKATE
        depends on PLATFORM_ARCH_ARMEL && PLATFORM_EXPERIMENTAL
        select CROS_EC_I2C
        select SOC_SAMSUNG_EXYNOS5250
        bool "skate"
        default n

endmenu
