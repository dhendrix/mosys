#
# Experimental platform configuration
#

menu "Experimental Platform Support"

config PLATFORM_EXPERIMENTAL
	bool "Experimental platform support"
	default n

config EXPERIMENTAL_AGZ
	depends on PLATFORM_ARCH_X86 && PLATFORM_EXPERIMENTAL
	select CHIPSET_INTEL_NM10
	bool "agz pinetrail reference board"
	default n

config EXPERIMENTAL_MARIO
	depends on PLATFORM_ARCH_X86 && PLATFORM_EXPERIMENTAL
	select CHIPSET_INTEL_NM10
	select EC_ITE_IT8500
	bool "mario pinetrail reference board"
	default n

endmenu
